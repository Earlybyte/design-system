{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "member-access": [
      true,
      "no-public"
    ],
    "interface-name": false,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}
