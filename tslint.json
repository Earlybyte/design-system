{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "member-access": [
      true,
      "no-public"
    ],
    "interface-name": false,
    "quotemark": [
      true,
      "double"
    ],
    "curly": false,
    "member-ordering": false,
    "arrow-parens": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ]
  },
  "rulesDirectory": []
}
